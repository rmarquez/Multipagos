<?xml version="1.0" encoding="UTF-8"?>
<fb:context xmlns:fb="http://apache.org/cocoon/forms/1.0#binding" path="/">
    <fb:value id="usrId" path="usrId"/>
    <fb:value id="fecha" path="fecha"/>

	<fb:repeater id="detalle" parent-path="." row-path="detalleGIbwList">
        <fb:identity>
            <fb:value id="carteraId" path="carteraId"/>
            <fb:value id="simboloId" path="simboloId"/>
            <fb:value id="colectorId" path="colectorId"/>
            <fb:value id="municipioId" path="municipioId"/>
            <fb:value id="barrioId" path="barrioId"/>
        </fb:identity>

        <fb:on-bind>
            <fb:value id="carteraId" path="carteraId"/>
            <fb:value id="municipioId" path="municipioId"/>
            <fb:value id="barrioId" path="barrioId"/>
            <fb:value id="simboloId" path="simboloId"/>
            <fb:value id="colectorId" path="colectorId"/>
            <fb:value id="codigo" path="codigo"/>
            <fb:value id="fechaGestion" path="fechaGestion"/>
            <fb:value id="avisoCobro" path="avisoCobro"/>
            <fb:value id="observaciones" path="observaciones"/>
            <fb:value id="horaRegistro" path="horaRegistro"/>
            <fb:value id="fprogCobro" path="fprogCobro"/>
            <fb:value id="gestionLlamada" path="gestionLlamada"/>
        </fb:on-bind>

        <fb:on-delete-row>
            <fb:delete-node/>
        </fb:on-delete-row>

        <fb:on-insert-row>
            <fb:insert-bean classname="com.metropolitana.multipagos.DetalleGIbw" addmethod="addDetalleGIbw"/>
        </fb:on-insert-row>
    </fb:repeater>
</fb:context>